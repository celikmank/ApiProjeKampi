<!DOCTYPE html>
<html lang="en">

  @await Component.InvokeAsync("_HeadDefaultComponentPartial")

<body class="index-page">

    @await Component.InvokeAsync("_NavbarDefaultComponentPartial")

  

    <main class="main">

        <!-- Hero Section -->
        @await Component.InvokeAsync("_FeatureDefaultComponentPartial")

        <!-- About Section -->
        @await Component.InvokeAsync("_AboutDashboardComponentPartial")

        <!-- Why Us Section -->
        @await Component.InvokeAsync("_ServiceDefaultComponentPartial")

        <!--Testimonial Section-->
        @await Component.InvokeAsync("_TestimonialDefaultComponentPartial")
        <!--Event Section-->
        @await Component.InvokeAsync("_EventDefaultComponentPartial")
        <!-- Stats Section -->
        <section id="stats" class="stats section dark-background">

            <img src="/Yummy/assets/img/stats-bg.jpg" alt="" data-aos="fade-in">

            <div class="container position-relative" data-aos="fade-up" data-aos-delay="100">

                <div class="row gy-4">

                    <div class="col-lg-3 col-md-6">
                        <div class="stats-item text-center w-100 h-100">
                            <span data-purecounter-start="0" data-purecounter-end="232" data-purecounter-duration="1" class="purecounter"></span>
                            <p>Clients</p>
                        </div>
                    </div><!-- End Stats Item -->

                    <div class="col-lg-3 col-md-6">
                        <div class="stats-item text-center w-100 h-100">
                            <span data-purecounter-start="0" data-purecounter-end="521" data-purecounter-duration="1" class="purecounter"></span>
                            <p>Projects</p>
                        </div>
                    </div><!-- End Stats Item -->

                    <div class="col-lg-3 col-md-6">
                        <div class="stats-item text-center w-100 h-100">
                            <span data-purecounter-start="0" data-purecounter-end="1453" data-purecounter-duration="1" class="purecounter"></span>
                            <p>Hours Of Support</p>
                        </div>
                    </div><!-- End Stats Item -->

                    <div class="col-lg-3 col-md-6">
                        <div class="stats-item text-center w-100 h-100">
                            <span data-purecounter-start="0" data-purecounter-end="32" data-purecounter-duration="1" class="purecounter"></span>
                            <p>Workers</p>
                        </div>
                    </div><!-- End Stats Item -->

                </div>

            </div>

        </section><!-- /Stats Section -->
        <!-- Menu Section -->
        @await Component.InvokeAsync("_DefaultMenuViewComponentPartial")

        
        @await Component.InvokeAsync("_TestimonialDefaultComponentPartial")
        <!-- Testimonials Section -->
        @await Component.InvokeAsync("_ChefDefaultComponentPartial")
        <!-- Chefs Section -->
        
        <!-- Gallery Section -->
        <section id="gallery" class="gallery section light-background">

            <!-- Section Title -->
            <div class="container section-title" data-aos="fade-up">
                <h2>Gallery</h2>
                <p><span>Check</span> <span class="description-title">Our Gallery</span></p>
            </div><!-- End Section Title -->

            <div class="container" data-aos="fade-up" data-aos-delay="100">

                <div class="swiper init-swiper">
                    <script type="application/json" class="swiper-config">
                        {
                          "loop": true,
                          "speed": 600,
                          "autoplay": {
                            "delay": 5000
                          },
                          "slidesPerView": "auto",
                          "centeredSlides": true,
                          "pagination": {
                            "el": ".swiper-pagination",
                            "type": "bullets",
                            "clickable": true
                          },
                          "breakpoints": {
                            "320": {
                              "slidesPerView": 1,
                              "spaceBetween": 0
                            },
                            "768": {
                              "slidesPerView": 3,
                              "spaceBetween": 20
                            },
                            "1200": {
                              "slidesPerView": 5,
                              "spaceBetween": 20
                            }
                          }
                        }
                    </script>
                    <div class="swiper-wrapper align-items-center">
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-1.jpg"><img src="~/Yummy/assets/img/gallery/gallery-1.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-2.jpg"><img src="~/Yummy/assets/img/gallery/gallery-2.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-3.jpg"><img src="~/Yummy/assets/img/gallery/gallery-3.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-4.jpg"><img src="~/Yummy/assets/img/gallery/gallery-4.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-5.jpg"><img src="~/Yummy/assets/img/gallery/gallery-5.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-6.jpg"><img src="~/Yummy/assets/img/gallery/gallery-6.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-7.jpg"><img src="~/Yummy/assets/img/gallery/gallery-7.jpg" class="img-fluid" alt=""></a></div>
                        <div class="swiper-slide"><a class="glightbox" data-gallery="images-gallery" href="~/Yummy/assets/img/gallery/gallery-8.jpg"><img src="~/Yummy/assets/img/gallery/gallery-8.jpg" class="img-fluid" alt=""></a></div>
                    </div>
                    <div class="swiper-pagination"></div>
                </div>

            </div>

        </section><!-- /Gallery Section -->
        <!-- Contact Section -->
        @await Component.InvokeAsync("_FooterDefaultComponentPartial")

    </main> 
</body>

</html>